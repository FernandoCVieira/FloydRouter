# EditorConfig is awesome: https://EditorConfig.org
# Edito Config do projeto de TCC - Floy Router

# top-most EditorConfig file
root = true

[*]
charset = utf-8
indent_style = space
indent_size = 4
end_of_line = lf
trim_trailing_whitespace = true
insert_final_newline = false
quote_type = single

# Boostrap
[*.md]
trim_trailing_whitespace = false
